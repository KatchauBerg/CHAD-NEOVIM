{
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "suggest.confirmKeys": [
    "<CR>"
  ],
  "suggest.acceptSuggestionOnCommitCharacter": false
}
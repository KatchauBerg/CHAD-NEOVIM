{
  "coc.preferences.formatOnSaveFiletypes": [
    "typescript",
    "typescriptreact",
    "javascript",
    "javascriptreact",
    "python"
  ],
  "eslint.autoFixOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "services": [
    {
      "id": "ts-standard",
      "command": "npx",
      "args": [
        "ts-standard",
        "--fix",
        "--stdin",
        "--stdin-filename",
        "%filepath"
      ],
      "filetypes": [
        "typescript",
        "typescriptreact"
      ]
    }
  ]
}
